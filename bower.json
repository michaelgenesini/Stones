{
  "name": "stones-css",
  "version": "0.1.0",
  "keywords": [
    "stones",
    "framework",
    "css",
    "sass"
  ],
  "ignore": [
    "docs/",
    "src/views",
    "node_modules",
    ".sass-cache",
    ".gitignore",
    "_config.yml",
    "**/.*",
    "docs/bower_components"
  ],
  "main": [
    "dist/css/stones.css",
    "dist/js/stones.js",
    "dist/imgs/*",
    "dist/fonts/*",
    "src/sass/*",
    "src/coffee/*"
  ],
  "dependencies": {
    "fontawesome": "~4.3.0",
    "jquery": "~2.1.3"
  },
  "homepage": "https://github.com/michaelgenesini/stones-css",
  "authors": [
    "Michael <michael.genesini@gmail.com>"
  ],
  "license": "MIT"
}
